-- Scripts:

local function VJMS_fake_script() -- Minimize.Mini 
	local script = Instance.new('LocalScript', Minimize)

	local Frame = script.Parent.Parent
	local Frame2 = script.Parent.Parent.Parent.Frame2
	
	script.Parent.MouseButton1Down:Connect(function()
		Frame2.Position = Frame.Position
		Frame.Visible = false
		Frame2.Visible = true
	end)
end
coroutine.wrap(VJMS_fake_script)()
local function UBFB_fake_script() -- Frame.UI 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.UIDragDetector.Enabled = true
	
end
coroutine.wrap(UBFB_fake_script)()
local function RZUPVU_fake_script() -- Frame.Backpack 
	local script = Instance.new('LocalScript', Frame)

	local player = game.Players.LocalPlayer
	local backpack = player:WaitForChild("Backpack")
	local itemList = script.Parent:WaitForChild("ItemList")
	
	-- Function to create buttons
	local function refreshButtons()
		-- Clear existing buttons
		for _, child in pairs(itemList:GetChildren()) do
			if child:IsA("TextButton") then
				child:Destroy()
			end
		end
	
		-- Create a button for each item
		for _, item in pairs(backpack:GetChildren()) do
			local button = Instance.new("TextButton")
			button.Size = UDim2.new(1, 0, 0.058, 0)
			button.Text = item.Name
			button.BackgroundColor3 = Color3.fromRGB(6, 0, 170)
			button.TextColor3 = Color3.new(0, 27, 71)
			button.Parent = itemList
	
			button.MouseButton1Click:Connect(function()
				local clone = item:Clone()
				clone.Parent = backpack
			end)
		end
	end
	
	-- Update on backpack changes
	backpack.ChildAdded:Connect(refreshButtons)
	backpack.ChildRemoved:Connect(refreshButtons)
	
	-- Initial render
	refreshButtons()
	
end
coroutine.wrap(RZUPVU_fake_script)()
local function XMIMMG_fake_script() -- Maximize.Max 
	local script = Instance.new('LocalScript', Maximize)

	local Frame2 = script.Parent.Parent
	local Frame = script.Parent.Parent.Parent.Frame
	
	Frame2.Position = Frame.Position
	Frame2.Visible = false
	
	script.Parent.MouseButton1Down:Connect(function()
		Frame.Position = Frame2.Position
		Frame.Visible = true
		Frame2.Visible = false
	end)
end
coroutine.wrap(XMIMMG_fake_script)()
local function EGGH_fake_script() -- Frame2.UI 
	local script = Instance.new('LocalScript', Frame2)

	script.Parent.UIDragDetector.Enabled = true
	
end
coroutine.wrap(EGGH_fake_script)()
